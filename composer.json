{
    "name": "labSystem",
    "description": "A system used for testing eid and viralload samples.",
    "keywords": ["eid", "viralload", "testing", "results"],
    "homepage": "http://lab.test.nascop.org",
    "license": "MIT",
    "type": "project",
    "prefer-stable": true,
    "minimum-stability": "dev", 
    "require": {
        "php": ">=7.1.3",
        "laravel/framework": "5.6.*",
        "dingo/api": "2.0.0-alpha1",
        "barryvdh/laravel-cors": "0.11.*",
        "laravel/tinker": "^1.0",
        "laravelcollective/html": "^5.6",
        "doctrine/dbal": "^2.8",
        "guzzlehttp/guzzle": "^6.3",
        "maatwebsite/excel": "^2.1",
        "venturecraft/revisionable": "^1.28",
        "milon/barcode": "^5.3",
        "predis/predis": "^1.1",
        "mpdf/mpdf": "^7.0",
        "fideloper/proxy": "^4.0",
        "tymon/jwt-auth": "1.0.*",
        "simplesoftwareio/simple-qrcode": "^2.0"
    },
    "require-dev": {
        "fzaninotto/faker": "~1.4",
        "mockery/mockery": "0.9.*",
        "phpunit/phpunit": "~5.7",
        "symfony/css-selector": "3.1.*",
        "symfony/dom-crawler": "3.1.*",
        "beyondcode/laravel-er-diagram-generator": "^1.2"
    },
    "autoload": {
        "classmap": [
            "database"
        ],
        "psr-4": {
            "App\\": "app/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "App\\": "tests/"
        }
    },
    "scripts": {
        "post-root-package-install": [
            "php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "php artisan key:generate",
            "php artisan jwt:secret -f"
        ],
        "post-install-cmd": [
            "Illuminate\\Foundation\\ComposerScripts::postInstall"
        ],
        "post-update-cmd": [
            "Illuminate\\Foundation\\ComposerScripts::postUpdate"
        ]
    },
    "config": {
        "preferred-install": "dist"
    }
}
